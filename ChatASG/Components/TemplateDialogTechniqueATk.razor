<MudDialog DefaultFocus="DefaultFocus.LastChild" Style="height:90%">
    <TitleContent>
        <MudText Typo="Typo.h6">
            <MudIconButton OnClick="Cancel" Color="Color.Primary" Icon="@Icons.Material.Filled.ArrowBack" aria-label="delete"></MudIconButton>

            Show   @Model.Name
        </MudText>
    </TitleContent>
    <DialogActions>
        
        
    </DialogActions>
    <DialogContent>
     <TemplateTechniqueATk Model="Model" />
    </DialogContent>
    
</MudDialog>
@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }
    [Parameter] public  TechniqueATk Model { set; get; }
     
    void Submit() => MudDialog.Close(DialogResult.Ok(true));
    void Cancel() => MudDialog.Cancel();
}